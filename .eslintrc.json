{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "no-console": "off",
        "no-param-reassign": "warn",
        "no-bitwise": "off",
        "no-mixed-operators": "off",
        "no-plusplus": "off",
        "no-continue": "off",
        "eqeqeq": "warn"
    }
}
